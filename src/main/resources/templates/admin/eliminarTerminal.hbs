{{#partial "contenido"}}
<h1>TERMINALES</h1>
{{#with terminal}}
<form action="/terminales/{{id}}/eliminar" method="POST">
		<div>
			<label for="mensaje">Esta seguro de que desea eliminar la terminal {{nombre}}?</label>
		</div>
		<table> 
			<tr><input class="btn btn-primary btn-lg" type="submit" value="Aceptar" style="height:40px; width:100px"></tr>
			<tr><input class="btn btn-default btn-lg" onClick="parent.location='/terminales'" value="Cancelar" style="height:40px; width:110px"></tr>
		</table>
	</form>
</div>
{{/with}}
{{/partial}}

{{>layoutAdmin.hbs}}
