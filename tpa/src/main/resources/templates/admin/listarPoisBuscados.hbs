{{#partial "contenido"}}
<h1>POIS</h1>
   <form action="/busquedas/verPois" method="POST">

		<table> 
			<tr><input class="btn btn-primary btn-lg" type="submit" value="Pois buscados"></tr>
		</table>
	</form>
</div>

<table class="table table-stripped">
	<tr>
		<th>Nombre</th>
		<th>Direccion</th>
		<th>Longitud</th>
    	<th>Latitud</th>
	</tr>

	{{#each pois}}
		<tr>
			<td>{{nombre}}</td>
			<td>{{mostrarDir direccion}}</td>
			<td>{{x}}</td>
     	    <td>{{y}}</td>
	{{/each}}
</table>
{{/partial}}

